#include <stdio.h>
/* prototypes of functions, first one compresses input while second expand input */
void compress();
void expand();

int main(void)
{
	/* read the input command */
	char command;
	scanf("%c",&command);
	
	if (command=='C')
	{
	    scanf("%c",&command);

		compress();
	}
	else if (command == 'E')
	{
		expand();
	}
	return 0;
}

void compress()
{
	/* intialize the char which stores previous char*/
	char old = '\0';
	/* declare the char which store current char*/
	char current;

	while(scanf("%c",&current)!=EOF)
	{
		    
		if(current==old)
		{
			printf("%c",current);
			int count = 2;
			while(1)
			{
				scanf("%c",&current);
				
				if (current!=old||current==EOF)
				{
					printf("%d*", count);
					printf("%c",current);
					old = current;
					
					break;
				}
				count++;				
			}
		}
		else
		{   
		    printf("%c",current);
			old = current;
		    
			
		}
	}

}

void expand(){
	/* intialize the char which stores previous char*/
	char old = '\0';
	/* declare the char which store current char*/
	char current;
	while(scanf("%c",&current)!=EOF)
	{
		if(current==old)
		{
			printf("%c",current);
			int repeat;
			scanf("%d",&repeat);
			repeat-=2;
			while(repeat--)
			{
				printf("%c",current);
			}
			char star;
			scanf("%c",&star);
		}
		else
		{
			printf("%c",current);
			old = current;
		}
	}
}
